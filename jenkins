pipeline{
  agent any

  tools{
      maven "maven_3.8.8"
  }
  stages{
    stage('mvn version'){
      step{
        echo "hello,Maven"
        sh "mvn --version"
      }
    }
    stage('mvn compile'){
      step{
        sh "mvn clean compile"
        echo "code compilation completed"
      }
    }
     stage(mvn package){
       step{
         sh "mvn clean package"
         echo "code artifact ready"
       }
     }
  }
}
